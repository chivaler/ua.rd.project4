<c:if test="${not empty entity}">
    <c:if test="${not empty entity.getId()}">
        <tr>
            <td><fmt:message key="id" bundle="${bundle}"/></td>
            <td><INPUT type=text class="readonly" readonly="readonly" name="id"
                       value="${empty entity ? '' : entity.getId()}"/>
            </td>
        </tr>
    </c:if>
</c:if>